n = int(input("Enter number of elements: "))
arr = []

for i in range(n):
    num = int(input(f"Enter element {i+1}: "))
    arr.append(num)
is_sorted = all(arr[i] <= arr[i+1] for i in range(n-1))

if is_sorted:
    print("Array is sorted in ascending order.")
else:
    print("Array is not sorted.")
